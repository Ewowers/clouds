<div class='btn-group mb-3'>
  <button class='btn btn-primary' id='fixed'>Сахранить</button>
</div>
<table class='table table-dark table-striped border'>
  <thead>
    <tr>
      <th scope='col'>i</th>
      <th scope='col'>Название</th>
      <th scope='col'>Цена</th>
      <th scope='col'>Количество</th>
      <th scope='col'>Тип</th>
      <th scope='col'></th>
    </tr>
  </thead>
  <tbody>
    {{#each product}}
      <tr>
        <th scope='row' class='isRow' id='{{_id}}'>{{@index}}</th>
        <td><input
            type='text'
            value='{{name}}'
            class='form-control ProductName'
          />
        </td>
        <td><input value='{{prise}}' class='form-control ProductPrise' /></td>
        <td><input value='{{que}}' class='form-control ProductQues' /></td>
        <td>
          <select class='form-control ProductType'>
            <option value='{{type}}'>{{type}}</option>
            {{#each line}}
              <option value='{{this}}'>{{this}}</option>
            {{/each}}
          </select>
        </td>
        <td>
          <button class='btn btn-danger deletes' data-id={{_id}}><i
              class='fas fa-times'
            ></i></button>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
<script src='/admin/isFixed.js'></script>
<script src='/admin/admin.js'></script>